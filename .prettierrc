{
  "printWidth": 120,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "always",
  "endOfLine": "lf",
  "jsdocParser": true,
  "overrides": [
    {
      "files": [
        "*.js",
        "*.cjs",
        "*.mjs"
      ],
      "options": {
        "parser": "babel"
      }
    },
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": [
        "*.json",
        "*.jsonc",
        ".babelrc",
        ".prettierrc"
      ],
      "options": {
        "parser": "json"
      }
    },
    {
      "files": [
        "*.yaml",
        "*.yml"
      ],
      "options": {
        "parser": "yaml"
      }
    },
    {
      "files": [
        "*.md"
      ],
      "options": {
        "parser": "markdown"
      }
    }
  ]
}