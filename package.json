{
  "name": "workflow-toggle",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "rimraf ./dist && ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StephenMP/workflow-toggle.git"
  },
  "keywords": [
    "actions",
    "node",
    "workflows"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0"
  },
  "devDependencies": {
    "@types/nock": "^11.1.0",
    "@types/node": "^16.10.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/parser": "^5.8.1",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.5.1",
    "js-yaml": "^4.1.0",
    "nock": "^13.2.4",
    "prettier": "2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4",
    "uuid": "^8.3.2"
  }
}
